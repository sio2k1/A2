        <Button x:Name="btnCategories" Content="Categories" HorizontalAlignment="Center" Margin="0,147,0,0" VerticalAlignment="Top" Height="70" Width="508" AutomationProperties.Name="btnCategories" Click="btnCategories_Click"/>

    </Grid>
</Window>
