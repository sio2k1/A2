<local:NPartContainer x:Class="WinClientWPF.PartsWired"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WinClientWPF"
             mc:Ignorable="d" HorizontalAlignment="Left" VerticalAlignment="Top" Height="66" Width="347">
    <Grid Margin="10,10,10,10">
        <Label x:Name="lbl1" Content="Ethernet port type:" HorizontalAlignment="Left" VerticalAlignment="Top" Width="155"/>
        <TextBox x:Name="tbEthernetPortType" Text="{Binding EthernetPortType, Mode = TwoWay, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="0,26,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="162"/>
        <TextBox x:Name="tbEthernetPortNumber" Text="{Binding NumberOfPorts, Mode = TwoWay, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="167,26,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
        <Label x:Name="lbl1_Copy" Content="Ethernet port number:" HorizontalAlignment="Left" Margin="167,0,0,0" VerticalAlignment="Top" Width="155"/>
    </Grid>
</local:NPartContainer>
